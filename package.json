{
  "name": "viewability-helper",
  "version": "1.0.5",
  "description": "Lightweight cross-browser library that enables you to create a watcher for a given element, and get back a callback when it has entered the viewport",
  "main": "./src/viewability-helper.js",
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch"
  },
  "author": "Liron Zluf",
  "license": "MIT",
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "4.1.3",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "webpack": "1.12.9"
  }
}
